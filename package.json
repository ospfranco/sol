{
  "name": "sol",
  "version": "2.1.32",
  "private": true,
  "scripts": {
    "start": "react-native-macos start",
    "macos": "react-native run-macos --scheme debug",
    "test": "jest",
    "bump": "./bump-version.sh",
    "permissions:reset": "tccutil reset Calendar com.ospfranco.sol && tccutil reset Accessibility com.ospfranco.sol",
    "dev": "bundle exec fastlane dev && rimraf releases/Sol.app && open /Applications/Sol.app",
    "release": "bundle exec fastlane release",
    "nuke": "npm i && rm -rf macos/build && rm -rf macos/Pods && npx pod-install macos",
    "fix-dependencies": "rnx-align-deps --write",
    "appcast": "./macos/Pods/Sparkle/bin/generate_appcast releases",
    "pods": "bundle exec pod install --project-directory=macos",
    "postinstall": "npm run pods && patch-package",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/plist": "^0.0.20",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-picker/picker": "^2.4.0",
    "@sentry/react-native": "5.19.2",
    "assert": "^2.0.0",
    "axios": "^0.25.0",
    "chance": "^1.1.9",
    "clsx": "^1.2.1",
    "expr-eval": "^2.0.2",
    "fuse.js": "^6.5.3",
    "immer": "^9.0.12",
    "intl": "^1.2.5",
    "lodash": "^4.17.21",
    "luxon": "^2.3.0",
    "mobx": "^6.3.12",
    "mobx-react-lite": "3.4.0",
    "nanoid": "^4.0.0",
    "nativewind": "2.0.11",
    "patch-package": "^6.5.1",
    "pretty-bytes": "^6.1.1",
    "react": "18.2.0",
    "react-native": "^0.73.1",
    "react-native-get-random-values": "^1.8.0",
    "react-native-macos": "^0.73.20",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.182",
    "@types/luxon": "^2.0.9",
    "@types/react": "^18.2.6",
    "@types/react-native": "^0.66.12",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^9.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.6.3",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "3.2.4",
    "typescript": "5.0.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "packageManager": "yarn@4.1.1"
}
